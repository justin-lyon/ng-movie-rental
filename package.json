{
  "name": "ng-movie-rental",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:all": "npm run lint:validate && npm test && npm run test:int",
    "test:int": "npm run test:int:chrome && npm run test:int:firefox",
    "test:int:chrome": "cypress run --headless --browser chrome --config video=false",
    "test:int:chromium": "cypress run --headless --browser chromium --config video=false",
    "test:int:firefox": "cypress run --headless --browser firefox --config video=false",
    "test:watch": "jest -o --watch",
    "test:coverage": "jest --coverage",
    "e2e": "npm run test && npm run test:cypress",
    "lint": "tsc --noEmit && eslint . --ext js,ts --quiet --fix && prettier --write .",
    "lint:validate": "tsc --noEmit && eslint ./src --ext js,ts --quiet && prettier --check ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@briebug/cypress-schematic": "^4.3.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "codelyzer": "^6.0.0",
    "cypress": "^8.7.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.2",
    "jest-preset-angular": "^8.3.2",
    "ng-mocks": "^11.1.3",
    "prettier": "^2.3.0",
    "prettier-eslint": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "8.0.7",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
